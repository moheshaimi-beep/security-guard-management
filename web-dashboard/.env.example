# Frontend Environment Variables - Security Guard Management

# API Backend URL
REACT_APP_API_URL=https://your-backend.onrender.com/api

# Socket.IO URL
REACT_APP_SOCKET_URL=https://your-backend.onrender.com

# Application Configuration
REACT_APP_APP_NAME=Security Guard Management
REACT_APP_ENV=production

# Disable source maps in production
GENERATE_SOURCEMAP=false

# Features
REACT_APP_ENABLE_FACIAL_RECOGNITION=true
REACT_APP_ENABLE_GEOLOCATION=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Face-API Model URL
REACT_APP_FACE_API_MODEL_URL=https://raw.githubusercontent.com/justadudewhohacks/face-api.js/master/weights

# Google Maps API (Optional)
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Build Optimization
INLINE_RUNTIME_CHUNK=false
DISABLE_NEW_JSX_TRANSFORM=false
